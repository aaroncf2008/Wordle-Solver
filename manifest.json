{
    "manifest_version": 3,
    "name": "Wordle Solver",
    "description": "Automatically Solve Wordle Puzzles",
    "version": "1.0.2",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [{
        "matches": ["*://www.powerlanguage.co.uk/wordle/*"],
        "js": ["js/content.js"],
        "run_at": "document_start"
    }]
}